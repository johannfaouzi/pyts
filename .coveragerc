# Configuration for coverage.py

[run]
branch = True
source = pyts
include = */pyts/*
omit =
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    raise TypeError
    raise ValueError
    if 0:
    if __name__ == .__main__.:
    if self.verbose:
show_missing = True
